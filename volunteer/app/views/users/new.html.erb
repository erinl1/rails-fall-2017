<body>
<h1> create volunteer account</h1>
  <div class = "container">
    <%= form_for(@user) do |form| %>
      <div class = "form-group">
      <%= form.label :name%>
      <%= form.text_field :name, class: 'form-control' %>
      </div>
      <div class = "form-group">
      <%= form.label :age%>
      <%= form.text_field :age, class: 'form-control' %>
    </div>
    <div class = "form-group">
      <%= form.label :username %>
      <%= form.text_field :username, class: 'form-control' %>
    </div>
    <div class = "form-group">
      <%= form.label :school %>
      <%= form.text_field :school, class: 'form-control' %>
    </div>
    <div class = "form-group">
      <%= form.label :password %>
      <%= form.text_field :password, class: 'form-control', type: 'password' %>
    </div>
    <div class = "form-group">
      <%= form.submit "Create Account", class: 'form-control btn btn-primary' %>
      <% end %>
  </div>
</div>
</body>
